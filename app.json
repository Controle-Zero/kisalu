{
  "expo": {
    "backgroundColor": "#fff",
    "name": "kisalu",
    "slug": "kisalu",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/appIcon.png",
    "splash": {
      "image": "./src/assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#fff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "buildNumber": "1.0.0",
      "bundleIdentifier": "com.controlezero.kisalu"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/appIcon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.controlezero.kisalu",
      "versionCode": 1
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "Kisalu requires photos permissions to upload your profile picture",
          "cameraPermission": "Kisalu requires your camera permissions to take a picture of your profile picture"
        }
      ]
    ]
  }
}
